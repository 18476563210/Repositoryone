page{
  height: 100%;
  background-color:rgb(250, 207, 214);
}
.container{
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color:rgb(250, 207, 214);
}
.user-avatar{
  width: 200rpx;
  height: 200rpx;
  margin-top: 160rpx;
}
.user-name{
  font-size: 32rpx;
  font-weight: bold;
}
.color{
  color: white;
  font-size: 88rpx;
}
.moto-container{
  margin-top: 200rpx;
  border: 1px solid #405f80;
  width: 200rpx;
  height: 80rpx;
  border-radius: 5rpx;
  text-align:center;
}
.moto{
  font-size: 22rpx;
  font-weight: bold;
  line-height: 80rpx;
  color: #405f80
}